$(document).ready(function(){
	if($(".pages.paid").length > 0){
		redirectTime = "10000";
		redirectURL = "<%= Rails.application.routes.url_helpers.root_path %>";

		$.ajax({
	    type: "DELETE",
	    url: "<%= Rails.application.routes.url_helpers.destroy_user_session_path %>"
		});

		setTimeout(function () {
	  	window.location.href = redirectURL;
		}, redirectTime);
	}
});