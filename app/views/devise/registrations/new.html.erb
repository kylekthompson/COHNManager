<h2>Sign up</h2>

 <%= semantic_form_for(resource, :as => resource_name, :url => registration_path(resource_name), html: {class: "form-vertical"}) do |f| %>
  <%= devise_error_messages! %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.actions do %>
    <%= f.action :submit %>
  <% end %>
<% end %>

<%= render "devise/shared/links" %>
